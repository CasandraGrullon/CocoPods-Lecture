# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'CocoPods-Lecture' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for CocoPods-Lecture
  pod 'Alamofire' # not specifying the pod version number will automatically install the latest version of the library
  
  #SnapKit is an Autolayout third party library
  pod 'SnapKit'
  
  #Kingfisher is a photo processing library
  pod 'Kingfisher'

end
